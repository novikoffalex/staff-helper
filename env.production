# Laravel Cloud Production Environment
NODE_ENV=production
PORT=8000

# Lark Bot Configuration (заполните в Laravel Cloud)
LARK_APP_ID=your_app_id_here
LARK_APP_SECRET=your_app_secret_here
LARK_WEBHOOK_URL=https://your-app-name.laravelcloud.com/webhook

# OpenAI Configuration (заполните в Laravel Cloud)
OPENAI_API_KEY=your_openai_api_key_here
OPENAI_MODEL=gpt-4

# Security (заполните в Laravel Cloud)
WEBHOOK_VERIFICATION_TOKEN=your_verification_token_here
